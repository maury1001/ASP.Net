@model List<Alumno>

@{
    ViewData["Title"] = "Informaci√≥n Alumno";
    Layout = "_Layout";
}


<h1>ViewData["Title"]</h1>

<strong>Desde la vista normal</strong>



<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th  scope ="col">Nombre</th>
            <th  scope ="col">ID</th>
        </tr>
        <tbody>
            @foreach (var alumno in Model)
            {
               <tr>
                   <td>@alumno.Nombre</td>
                   <td>@alumno.UniqueId</td>
               </tr>
            }
        </tbody>
    </thead>
</table>






